{
  "name": "@reeba/common",
  "version": "1.0.0",
  "private": true,
  "description": "Common functions for ReebA",
  "main": "dist/index",
  "types": "dist/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/grindarius/reeba.git",
    "directory": "common"
  },
  "author": "ReebA Team",
  "scripts": {
    "serve": "node dist/index.js",
